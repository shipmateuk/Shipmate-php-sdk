{
    "name": "shipmate/shipmate-php-sdk",
    "description": "Shipmate API SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "Shipmate",
            "email": "info@shipmate.co.uk"
        }
    ],
    "homepage": "https://www.shipmate.co.uk/",
    "support": {
        "email": "support@shipmate.co.uk"
    },
    "keywords": [
        "shipmate",
        "sdk",
        "shipping",
        "php",
        "composer",
        "package",
        "library"
    ],
    "type": "library",
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Shipmate\\": "src/"
        },
        "psr-0": {
            "Shipmate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shipmate\\Shipmate\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    }
}
